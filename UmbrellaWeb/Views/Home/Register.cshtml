
@{
    ViewBag.Title = "Register";
}

<div class="panel panel-primary">
    <div class="panel-heading">Refer A Friend</div>
    <div class="panel-body">
       <h4 style="text-align:center">Give our Welcome Team a call today for an in depth quote</h4>
        <br />
        <div class="form-group row">
            <label for="txtname" class="col-md-2 col-md-form-label">Full Name</label>
            <div class="col-md-10">
                <input class="form-control" type="text" placeholder="Full Name" id="txtname">
            </div>
        </div>
        <div class="form-group row">
            <label for="txtDOB" class="col-md-2 col-md-form-label">DOB</label>
            <div class="col-md-10">
                <input class="form-control" type="text" placeholder="DOB" id="txtDOB">
            </div>
        </div>
        <div class="form-group row">
            <label for="txtEmail" class="col-md-2 col-md-form-label">Email</label>
            <div class="col-md-10">
                <input class="form-control" type="text" placeholder="Email" id="txtEmail">
            </div>
        </div>
        <div class="form-group row">
            <label for="txtNumber" class="col-md-2 form-label">Telephone</label>
            <div class="col-md-10">
                <input class="form-control" type="text" placeholder="Telephone" id="txtNumber">
            </div>
        </div>
        <div class="form-group row">
            <label for="txtAgency" class="col-md-2 col-md-form-label">Full Address</label>
            <div class="col-md-10">
                <textarea class="form-control" cols="15" rows="5"  placeholder="Full Address" id="txtaddress" ></textarea>
            </div>
        </div>
        <div class="form-group row">
            <label for="txtni" class="col-md-2 col-md-form-label">National Insurance</label>
            <div class="col-md-10">
                <input class="form-control" type="text" placeholder="National Insurance" id="txtni">
            </div>
        </div>
        <div class="form-group row">
            <label for="txtbank" class="col-md-2 col-md-form-label">Name of bank</label>
            <div class="col-md-10">
                <input class="form-control" type="text" placeholder="Name of bank" id="txtbank">
            </div>
        </div>
        <div class="form-group row">
            <label for="txtsortcode" class="col-md-2 col-md-form-label">Sort code</label>
            <div class="col-md-10">
                <input class="form-control" type="text" placeholder="Sort code" id="txtsortcode">
            </div>
        </div>
        <div class="form-group row">
            <label for="txtaccno" class="col-md-2 col-md-form-label">Account number</label>
            <div class="col-md-10">
                <input class="form-control" type="text" placeholder="Account number" id="txtaccno">
            </div>
        </div>
        <div class="form-group row">
            <label for="txtNationality" class="col-md-2 col-md-form-label">Nationality</label>
            <div class="col-md-10">
                <input class="form-control" type="text" placeholder="Nationality" id="txtNationality">
            </div>
        </div>
        <div class="form-group row">
            <label for="txtRate" class="col-md-2 col-md-form-label">Rate</label>
            <div class="col-md-10">
                <input class="form-control" type="text" placeholder="Rate" id="txtRate">
            </div>
        </div>
        <div class="form-group row">
            <label for="txtcon" class="col-md-2 col-md-form-label">Length of contract</label>
            <div class="col-md-10">
                <input class="form-control" type="text" placeholder="Length of contract" id="txtcon">
            </div>
        </div>
        <div class="form-group row">
            <label for="txtAgency" class="col-md-2 col-md-form-label">Agency</label>
            <div class="col-md-10">
                <input class="form-control" type="text" placeholder="Name" id="txtAgency">
            </div>
        </div>
        <div class="form-group row">
            <label for="txtagencycon" class="col-md-2 col-md-form-label">Agency contact</label>
            <div class="col-md-10">
                <input class="form-control" type="text" placeholder="Agency contact" id="txtagencycon">
            </div>
        </div>
        <div class="form-group row">
            <label for="txtOccupation" class="col-md-2 col-md-form-label">Occupation</label>
            <div class="col-md-10">
                <input class="form-control" type="text" placeholder="Occupation" id="txtOccupation">
            </div>
        </div>
        <div class="form-group row">
            <div class="col-md-12">
                <input id="btnSubmit" class="btn btn-primary" type="submit" value="submit" />
                <input id="Reset1" class="btn btn-info" type="reset" value="reset" />
            </div>
        </div>
    </div>
</div>

